#include<iostream>
using namespace std;
int main(int argc, char *argv[])
{
    
 long int a,b,c;
 for(int i=1;i<=30000;i++)
 {
  cin>>a>>b>>c;
   if(a==b && b==c &&c==0) {
   cout<<"";
  break;
   }
  else{
  if((a*a+b*b==c*c) || (a*a+c*c==b*b) || (b*b+c*c==a*a)) cout<<"right"<<endl;
  else cout<<"wrong"<<endl;
  }
 }
}
