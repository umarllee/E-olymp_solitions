#include <iostream>
#include <iomanip>
using namespace std;
 
int main()
{
    int n;
    cin >> n;
    double x[n];
    for (int i = 0; i < n; i++) {
        cin >> x[i];
        if (x[i] <= 2.5){
       cout<<i+1<<" "<<fixed<<setprecision(2)<<x[i]<<endl;
       return 0;
    }
    
    }
   cout<<"Not Found";
   return 0;
}
