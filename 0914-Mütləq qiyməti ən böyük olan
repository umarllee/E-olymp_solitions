#include <iostream>
#include <iomanip>
#include <math.h>
using namespace std;

int main() 
{
	int n;
	cin>>n;
	double a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
	
	double mx=a[0];
     for(int i=0;i<n;i++){
     	a[i]=abs(a[i]);
     	if(a[i]>mx) mx=a[i];
	 }
   cout<<fixed<<setprecision(2)<<mx;

}
